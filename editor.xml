<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
    <option name="COLLAPSE_FILE_HEADER" value="false" />
    <option name="COLLAPSE_DOC_COMMENTS" value="true" />
  </component>
  <component name="CodeInsightSettings">
    <option name="completionCaseSensitive" value="1" />
    <option name="selectAutopopupSuggestionsByChars" value="true" />
    <option name="PARAMETER_INFO_DELAY" value="3000" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="JAVADOC_INFO_DELAY" value="300" />
    <option name="COMPLETION_CASE_SENSITIVE" value="1" />
    <option name="SELECT_AUTOPOPUP_SUGGESTIONS_BY_CHARS" value="true" />
    <option name="SHOW_FULL_SIGNATURES_IN_PARAMETER_INFO" value="true" />
    <option name="REFORMAT_ON_PASTE" value="1" />
    <option name="INDENT_TO_CARET_ON_PASTE" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="LINE_SPACING" value="1.0" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Fira Code Light" />
  </component>
  <component name="EditorSettings">
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="PhpCodeFoldingSettings">
    <option name="COLLAPSE_HERE_DOCS" value="true" />
    <option name="COLLAPSE_IMPORTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>