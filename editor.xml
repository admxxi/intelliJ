<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
    <option name="COLLAPSE_METHODS" value="true" />
  </component>
  <component name="CodeInsightSettings">
    <option name="selectAutopopupSuggestionsByChars" value="true" />
    <option name="PARAMETER_INFO_DELAY" value="1200" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="JAVADOC_INFO_DELAY" value="1200" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="SELECT_AUTOPOPUP_SUGGESTIONS_BY_CHARS" value="true" />
    <option name="SHOW_FULL_SIGNATURES_IN_PARAMETER_INFO" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="LINE_SPACING" value="1.0" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Fira Code" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="CSS" value="true" />
        <entry key="HTML" value="false" />
        <entry key="Haml" value="true" />
        <entry key="JSON" value="true" />
        <entry key="JavaScript" value="true" />
        <entry key="LESS" value="true" />
        <entry key="PHP" value="true" />
        <entry key="SASS" value="true" />
        <entry key="SCSS" value="true" />
        <entry key="SQL" value="true" />
        <entry key="TypeScript" value="true" />
        <entry key="Vue" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
  </component>
  <component name="SqlCodeCompletionSettings" AUTO_ALIAS_TABLE="true">
    <option name="autoAliasTable" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>